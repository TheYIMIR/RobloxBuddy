<Application x:Class="RobloxBuddy.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RobloxBuddy"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <!-- Only basic colors -->
            <SolidColorBrush x:Key="RobloxRed" Color="#FF0000"/>
            <SolidColorBrush x:Key="RobloxGreen" Color="#02B757"/>
            <SolidColorBrush x:Key="RobloxLightGray" Color="#F2F4F5"/>
            <SolidColorBrush x:Key="RobloxDarkGray" Color="#393B3D"/>
            <SolidColorBrush x:Key="RobloxText" Color="#393B3D"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>